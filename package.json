{
  "name": "landing",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "./node_modules/.bin/husky install",
    "build:nexus-typegen": "ts-node --transpile-only src/modules/graphql/schema.ts",
    "predev": "yarn build:nexus-typegen && yarn generate",
    "dev": "./node_modules/.bin/next dev",
    "prebuild": "yarn build:nexus-typegen && yarn generate",
    "build": "./node_modules/.bin/next build",
    "start": "./node_modules/.bin/next start",
    "lint": "./node_modules/.bin/next lint",
    "prettier": "./node_modules/.bin/prettier --write \"{src,__{tests,mocks}__}/**/*{.js,.ts,.tsx}\"",
    "docker-build": "docker build -t sacrill-nextjs .",
    "docker-run": "docker run -p 3000:3000 sacrill-nextjs",
    "docker-images": "docker images",
    "analyze": "./node_modules/.bin/cross-env ANALYZE=true yarn build && serve .next/analyze",
    "sitemap": "./node_modules/.bin/cross-env SITE_URL=https://sacrill.com ./node_modules/.bin/next-sitemap",
    "generate": "graphql-codegen --config codegen.yml",
    "prisma-studio": "npx prisma studio",
    "db-pull": "npx prisma db pull",
    "prisma-generate": "npx prisma generate"
  },
